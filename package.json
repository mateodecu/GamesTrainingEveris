{
  "name": "angular-starter",
  "version": "1.2.0",
  "description": "Angular starter kit based on Angular CLI",
  "private": true,
  "scripts": {
    "build-dev": "ng build --configuration=development",
    "build-pre": "ng build --configuration=preproduction",
    "build-pro": "ng build --prod",
    "lint-api": "eslint src/api/**/*.js",
    "lint-css": "stylelint src/**/*{.css,.scss}",
    "lint-app": "ng lint",
    "lint": "npm run lint-api && npm run lint-css && npm run lint-app",
    "test-app": "ng test --watch=false",
    "test": "npm run lint && npm run test-app",
    "start": "pm2 start ecosystem.config.js",
    "stop": "pm2 delete ecosystem.config.js",
    "logs": "pm2 logs",
    "coverage": "serve --no-clipboard --listen 5000",
    "prettier-format": "prettier --write src/**/*{.md,.json,.html,.js,.ts,.css,.scss}",
    "check-uncommitted": "git diff --quiet && git diff --cached --quiet"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run check-uncommitted && npm test"
    }
  },
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/localize": "^9.1.9",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "core-js": "^2.6.11",
    "interpolate-url": "0.0.2",
    "lodash": "^4.17.15",
    "rxjs": "~6.5.5",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.7",
    "@angular/cli": "^9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@babel/core": "^7.10.2",
    "@types/jasmine": "^3.5.10",
    "@types/node": "^12.12.43",
    "babel-loader": "^8.1.0",
    "codelyzer": "^5.1.2",
    "eslint": "^5.12.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.19.1",
    "husky": "^4.2.5",
    "jasmine-core": "^3.5.0",
    "jasmine-spec-reporter": "^5.0.1",
    "karma": "^5.0.2",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^3.1.1",
    "lowdb": "^1.0.0",
    "morgan": "^1.10.0",
    "pm2": "^4.4.0",
    "prettier": "^1.19.1",
    "serve": "^11.3.0",
    "stylelint": "^13.6.0",
    "stylelint-config-standard": "^20.0.0",
    "ts-node": "~8.0.2",
    "tslint": "~6.1.0",
    "typescript": "~3.8.3"
  }
}
